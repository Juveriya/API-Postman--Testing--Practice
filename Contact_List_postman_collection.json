				{
					"name": "Add Contact",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"//pm.test(\"Status code is 200\", function () {\r",
									"//    pm.expect(pm.response.code).to.equal(200);\r",
									"//});\r",
									"\r",
									"// Set environment variables for the contact details\r",
									"const contactDetails = pm.response.json();\r",
									"pm.environment.set(\"contactid\", contactDetails._id);\r",
									"pm.environment.set(\"ContactFirstName\", contactDetails.firstName);\r",
									"pm.environment.set(\"ContactLastName\", contactDetails.lastName);\r",
									"pm.environment.set(\"ContactEmail\", contactDetails.email);\r",
									"pm.test(\"Status code is 201\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"});\r",
									"pm.test(\"Contact created successfully\", function () {\r",
									"    pm.expect(contactDetails.firstName).to.equal(pm.environment.get(\"ContactFirstName\"));\r",
									"    pm.expect(contactDetails.lastName).to.equal(pm.environment.get(\"ContactLastName\"));\r",
									"    pm.expect(contactDetails.email).to.equal(pm.environment.get(\"ContactEmail\"));\r",
									"});\r",
									"pm.test(\"Successful POST request\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
									"});\r",
